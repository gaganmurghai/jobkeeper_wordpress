.breadcrumb-block .breadcrumb-right{text-align: center ;}
.mysatff-account-photo-outer .coman-height{min-height: auto !important;height: auto!important;}
.registration-fields .add-plus{ right: -2px;top: -19px;}
.element-photo-outer .weekdays div .checkbox-custom, .element-photo-outer .weekdays div .checkbox-custom-label{width: auto;}
.registration-wrap .registration-btn-wrap .btn-normal{white-space: normal;padding: 20px;}
#close-snavvy{top: 0;margin-top: 0;}
.mystaff-account-info, .mysatff-account-photo, .mystaff-qualification, .mystaff-contact-info{height: inherit !important;min-height: inherit !important;}
.registration-fields .calender-plus{    right: 0px;  top: -22px;}
.payment-logo .list-inline {position: inherit;}
.tob-message{ margin-bottom: 30px;}
#mobile-menu{top: 53px;}
.mobile-account-login{top: 43px;}
.our-partner h3{font-size: 30px;}
}

@media only screen and (max-width: 640px) {
  .partner-img ul li {
    float: none;
    list-style: none;
    margin-bottom: 17px;
}
}


@media screen and (max-width: 500px) {
  .about-us-bg{padding: 30px 15px;}
  .breadcrumb-block h1, .breadcrumb-block h1{font-size: 28px;}
}
@media screen and (max-width: 420px) {
   .wrapper .logo-outer svg{margin: -36px auto 0 auto; max-width: 170px;}
   .template-mobile-menu .admin-menu-wrap{padding-left: 0;}
   .search-inner h1{font-size: 30px;}
   .partner-img{width: 100%;margin-bottom: 25px;}
}


 }



