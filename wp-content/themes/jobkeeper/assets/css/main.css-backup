.material-switch3 > label::after {
  background: #8d8b9c;
  border-radius: 16px;
  box-shadow: none;
  content: '';
  height: 24px;
  left: -4px;
  margin-top: -8px;
  position: absolute;
  top: -4px;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  -webkit-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
  width: 24px; }

.material-switch3 > input[type="checkbox"]:checked + label::before {
  background: inherit;
